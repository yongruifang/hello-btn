{
  "name": "@yonngrui/hello-btn",
  "private": false,
  "version": "0.0.1",
  "description": "easy button library",
  "author": "yongrui",
  "license": "MIT",
  "keywords": [
    "vue",
    "button"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yongruifang/hello-btn"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/hello-btn.umd.cjs",
  "module": "./dist/hello-btn.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/hello-btn.js",
      "require": "./dist/hello-btn.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build:only": "vite build",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "@vitejs/plugin-vue": "^4.6.2",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-style-inject": "^0.0.1",
    "vue-tsc": "^1.8.25"
  }
}
